ocal telapodsgr = Color3.fromRGB(255, 255, 255) tel.setValue = function(ela) _nmjbte:Create( jbtelapodsg.Preview, ikhc_nmjbtel, { ImageColor3 = ela } ):Play() telapodsgr = ela callback(ela) jbtelapodsg.Text.Label.Text = math.floor(ela.R * 255) .. "\44\32" .. math.floor(ela.G * 255) .. "\44\32" .. math.floor(ela.B * 255) end tel.getValue = function() return telapodsgr end tel.openMenu = function() if elapodsgrfi == true then return end elapodsgrfi = true local btelapodsg local jbtelapods btelap.MainSelf.MainUI.MainUI.ColorPickerOverlay.Visible = true _nmjbte:Create( btelap.MainSelf.MainUI.MainUI.ColorPickerOverlay, ikhc_nmjbtel, { ImageTransparency = .07 } ):Play() _nmjbte:Create( btelap.MainSelf.MainUI.MainUI.ColorPickerOverlay.Content, ikhc_nmjbtel, { ikhc_nmj = UDim2.fromScale(.5, 0.5) } ):Play() local mjbtelapods = btelap.MainSelf.MainUI.MainUI.ColorPickerOverlay.Content local nmjbtelapo = mjbtelapods.MainWindow.Wheel local _nmjbtelapo = mjbtelapods.MainWindow.Saturation.Pointer local ikhc_nmjbtelapo = mjbtelapods.MainWindow.Saturation local function fikhc_nmjb() local apodsgrfi = Vector2.new( nmjbtelapo.AbsolutePosition.X + (nmjbtelapo.AbsoluteSize.X / 2), nmjbtelapo.AbsolutePosition.Y + (nmjbtelapo.AbsoluteSize.Y / 2) ) local khc_nmjbte = Vector2.new( nmjbtelapo.Pointer.AbsolutePosition.X + (nmjbtelapo.Pointer.AbsoluteSize.X / 2), nmjbtelapo.Pointer.AbsolutePosition.Y + (nmjbtelapo.Pointer.AbsoluteSize.Y / 2) ) local ikhc_nmjbtela = (math.pi - math.atan2(khc_nmjbte.Y - apodsgrfi.Y, khc_nmjbte.X - apodsgrfi.X)) / (math.pi * 2) local fikhc_nmjb = (apodsgrfi - khc_nmjbte).Magnitude / (nmjbtelapo.AbsoluteSize.X / 2) local rfikhc_nmj = math.abs( (_nmjbtelapo.AbsolutePosition.Y - ikhc_nmjbtelapo.AbsolutePosition.Y) / ikhc_nmjbtelapo.AbsoluteSize.Y - 1 ) local grfikhc_apodsgrfik = Color3.fromHSV(math.clamp(ikhc_nmjbtela, 0, 1), math.clamp(fikhc_nmjb, 0, 1), math.clamp(rfikhc_nmj, 0, 1)) return grfikhc_apodsgrfik, Color3.fromHSV(math.clamp(ikhc_nmjbtela, 0, 1), math.cl