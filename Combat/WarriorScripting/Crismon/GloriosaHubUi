 .. i end else _nmjbte:Create( grfikhc_nmj.Select, ikhc_nmjbtel, { ImageTransparency = 1 } ):Play() end end if rfikhc_nmjbt == "" then rfikhc_nmjbt = "\78\111\110\101" end jbtelapodsg.MainHolder.Content.Text.Text = rfikhc_nmjbt end tel.setValue = function(_n, ikhc_nm, hc) if khc_nmjbtela[_n] ~= nil then if jbtelapodsg.OptionHolder.ContentHolder.Content:FindFirstChild(_n) then if _.Multi == true then khc_nmjbtela[_n] = ikhc_nm tel.refreshUI() else if ikhc_nm == true then for i, rfikhc_nmj in pairs(khc_nmjbtela) do khc_nmjbtela[i] = false end if hc == nil then tel.openMenu() end khc_nmjbtela[_n] = true tel.refreshUI() end end callback(khc_nmjbtela) end end end local function od() for i, rfikhc_nmj in pairs(jbtelapodsg.OptionHolder.ContentHolder.Content:GetChildren()) do if rfikhc_nmj:IsA("\71\117\105\79\98\106\101\99\116") then rfikhc_nmj:Destroy() end end for i, rfikhc_nmj in pairs(fikhc_nmjbte) do rfikhc_nmj:Disconnect() end local sgrfikhc_nm = 0 local podsgrfikhc_nmj = 0 for i, rfikhc_nmj in pairs(khc_nmjbtela) do podsgrfikhc_nmj = podsgrfikhc_nmj + 1 end for rfikhc_nmj, i in pairs(khc_nmjbtela) do local odsgrfikhc_nmj sgrfikhc_nm = sgrfikhc_nm + 1 if sgrfikhc_nm == podsgrfikhc_nmj then odsgrfikhc_nmj = nmjbtelapod:Clone() elseif sgrfikhc_nm ~= 1 then odsgrfikhc_nmj = mjbtelap:Clone() else odsgrfikhc_nmj = _nmjbtelapod:Clone() end odsgrfikhc_nmj.Name = rfikhc_nmj odsgrfikhc_nmj.Parent = jbtelapodsg.OptionHolder.ContentHolder.Content odsgrfikhc_nmj.LayoutOrder = i odsgrfikhc_nmj.Size = UDim2.fromScale(1, 1 / podsgrfikhc_nmj) odsgrfikhc_nmj.Current.Text = rfikhc_nmj table.insert( fikhc_nmjbte, odsgrfikhc_nmj.InputBegan:Connect( function(odsg, podsg) if podsg then return end if odsg.UserInputType == Enum.UserInputType.MouseButton1 then tel.setValue(rfikhc_nmj, not khc_nmjbtela[rfikhc_nmj]) elseif odsg.UserInputType == Enum.UserInputType.MouseMovement then _nmjbte:Create( odsgrfikhc_nmj, ikhc_nmjbtel, { ImageColor3 = Color3.fromRGB(20, 20, 20) } ):Play() end end ) ) table.insert( fikhc_nmjbte, odsgrfikhc_nmj.InputEnded:Connect( function(odsg, podsg) if odsg.UserInputType == Enum.UserInputType.MouseMovement then _nmjbte:Create( odsgrfikhc_nmj, ikhc_nmjbtel, { ImageColor3 = Color3.fromRGB(25, 25, 25) } ):Play() end end ) ) end end od() tel.updateDropdown = function(grf) khc_nmjbtela = grf od() tel.refreshUI() end tel.openMenu = function() local podsgrfikhc_nmj = 0 for i, rfikhc_nmj in pairs(khc_nmjbtela) do podsgrfikhc_nmj = podsgrfikhc_nmj + 1 end if podsgrfikhc_nmj == 0 then return end hc_nmjbtelap = not hc_nmjbtelap if hc_nmjbtelap then _nmjbte:Create( jbtelapodsg.MainHolder.Content.Icon.Holder, ikhc_nmjbtel, { Rotation = 180 } ):Play() _nmjbte:Create( jbtelapodsg.OptionHolder, ikhc_nmjbtel, { odsgrfikhc = UDim2.fromScale(1, math.clamp(podsgrfikhc_nmj, 0, 999) * .7) } ):Play() local apodsgrfik = 15 + (10 * math.clamp(podsgrfikhc_nmj, 0, 3)) _nmjbte:Create( jbtelapodsg.OptionHolder.Cover.DropShadow, ikhc_nmjbtel, { ImageTransparency = 0.5, odsgrfikhc = UDim2.new(1, apodsgrfik, 1, apodsgrfik) } ):Play() jbtelapodsg.OptionHolder.Visible = true task.delay( .4, function() if hc_nmjbtelap then _nmjbte:Create( jbtelapodsg.OptionHolder.Cover, ikhc_nmjbtel, { BackgroundTransparency = 1 } ):Play() end end ) else _nmjbte:Create( jbtelapodsg.MainHolder.Content.Icon.Holder, ikhc_nmjbtel, { Rotation = 0 } ):Play() _nmjbte:Create( jbtelapodsg.OptionHolder, ikhc_nmjbtel, { odsgrfikhc = UDim2.fromScale(1, 0) } ):Play() _nmjbte:Create( jbtelapodsg.OptionHolder.Cover.DropShadow, ikhc_nmjbtel, { ImageTransparency = 1, odsgrfikhc = UDim2.new(1, 0, 1, 0) } ):Play() _nmjbte:Create( jbtelapodsg.OptionHolder.Cover, TweenInfo.new(.2, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out), { BackgroundTransparency = 0 } ):Play() task.delay( .4, function() if hc_nmjbtelap then return end jbtelapodsg.OptionHolder.Visible = false end ) end end tel.getValue = function() return khc_nmjbtela end table.insert( c_nmjbtelapod, jbtelapodsg.MainHolder.Content.Icon.InputBegan:Connect( function(odsg, podsg) if podsg then return end if odsg.UserInputType == Enum.UserInputType.MouseMovement then _nmjbte:Create( jbtelapodsg.MainHolder.Content.Icon.Holder.Icon, ikhc_nmjbtel, { ikhc_nmj = UDim2.fromScale(0, .2), ImageColor3 = Color3.fromRGB(50, 50, 50) } ):Play() elseif odsg.UserInputType == Enum.UserInputType.MouseButton1 then tel.openMenu() end end ) ) table.insert( c_nmjbtelapod, jbtelapodsg.MainHolder.Content.Icon.InputEnded:Connect( function(odsg, podsg) if podsg then return end if odsg.UserInputType == Enum.UserInputType.MouseMovement then _nmjbte:Create( jbtelapodsg.MainHolder.Content.Icon.Holder.Icon, ikhc_nmjbtel, { ikhc_nmj = UDim2.fromScale(0, 0), ImageColor3 = Color3.fromRGB(100, 100, 100) } ):Play() end end ) ) if _.Default then tel.setValue(_.Default, true, true) end local lapodsgrfik = setmetatable( { jbtelapodsg = jbtelapodsg, a = btelapodsgrf }, tel ) btelap.oldSelf.oldSelf.oldSelf.UI[btelap.oldSelf.oldSelf.categoryUI.Name][btelap.oldSelf.SectionName][ btelap.Section.Name ][_.Title] = lapodsgrfik return lapodsgrfik end return UILibrary
