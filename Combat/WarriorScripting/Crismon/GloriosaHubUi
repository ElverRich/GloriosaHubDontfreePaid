elseif odsg.UserInputType == Enum.UserInputType.MouseButton1 then _nmjbte:Create(kh, ikhc_nmjbtel, ik(.5)):Play() lapodsgr:Fire() end end ) return lapodsgr.Event end function apo.Section:pods(_, callback) local tel = {} tel.__index = tel local btelapodsgrf = m("\66\117\116\116\111\110", _) btelapodsgrf.Parent = btelap.Section.Border.Content btelapodsgrf.LayoutOrder = p(btelap.Section.Border.Content) local jbtelapodsg = btelapodsgrf.Content.ElementContent.Button c_(jbtelapodsg, jbtelapodsg.HoverFrame):Connect( function() callback() end ) jbtelapodsg.Text.Text = _.ButtonName local lapodsgrfik = setmetatable( { jbtelapodsg = jbtelapodsg, a = btelapodsgrf }, tel ) btelap.oldSelf.oldSelf.oldSelf.UI[btelap.oldSelf.oldSelf.categoryUI.Name][btelap.oldSelf.SectionName][ btelap.Section.Name ][_.Title] = lapodsgrfik return lapodsgrfik end function apo.Section:Checkbox(_, callback) local tel = {} tel.__index = tel local btelapodsgrf = m("\67\104\101\99\107\98\111\120", _) btelapodsgrf.Parent = btelap.Section.Border.Content btelapodsgrf.LayoutOrder = p(btelap.Section.Border.Content) local jbtelapodsg = btelapodsgrf.Content.ElementContent.Checkbox local grfikhc = false tel.setValue = function(grf) grfikhc = grf if grf then _nmjbte:Create( jbtelapodsg.Selection, ikhc_nmjbtel, { odsgrfikhc = UDim2.fromScale(.85, .85), BackgroundTransparency = 0 } ):Play() else _nmjbte:Create( jbtelapodsg.Selection, ikhc_nmjbtel, { odsgrfikhc = UDim2.fromScale(0.5, 0.5), BackgroundTransparency = 1.1 } ):Play() end callback(grfikhc) end tel.getValue = function() return grfikhc end c_(jbtelapodsg, jbtelapodsg.HoverFrame):Connect( function() tel.setValue(not grfikhc) end ) if _.Default then tel.setValue(_.Default) end local lapodsgrfik = setmetatable( { jbtelapodsg = jbtelapodsg, a = btelapodsgrf }, tel ) btelap.oldSelf.oldSelf.oldSelf.UI[btelap.oldSelf.oldSelf.categoryUI.Name][btelap.oldSelf.SectionName][ btelap.Section.Name ][_.Title] = lapodsgrfik return lapodsgrfik end function apo.Section:Toggle(_