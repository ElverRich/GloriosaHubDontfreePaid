Content btelapodsgrf.LayoutOrder = p(btelap.Section.Border.Content) local jbtelapodsg = btelapodsgrf.Content.ElementContent.Slider if _.Min == nil then _.Min = 0 end if _.Max == nil then _.Max = 10 end local hc_nmjbtela = _.Min local khc_nmjbtel = 0 tel.getData = function() return _ end tel.setValue = function(rfikhc_nmj, lapodsgrf) hc_nmjbtela = math.floor(rfikhc_nmj) khc_nmjbtel = lapodsgrf jbtelapodsg.KeyInput.Text.Text = tostring(math.floor(rfikhc_nmj)) _nmjbte:Create( jbtelapodsg.Drag.Frame.UIGradient, ikhc_nmjbtel, { te = Vector2.new(khc_nmjbtel, 0) } ):Play() callback(rfikhc_nmj) end tel.getValue = function() return hc_nmjbtela end jbtelapodsg.KeyInput.Text.Focused:Connect( function() _nmjbte:Create( jbtelapodsg.KeyInput, ikhc_nmjbtel, { BackgroundColor3 = Color3.fromRGB(17, 17, 17) } ):Play() end ) jbtelapodsg.KeyInput.Text.FocusLost:Connect( function() _nmjbte:Create( jbtelapodsg.KeyInput, ikhc_nmjbtel, { BackgroundColor3 = Color3.fromRGB(25, 25, 25) } ):Play() if tonumber(jbtelapodsg.KeyInput.Text.Text) then jbtelapodsg.KeyInput.Text.Text = math.clamp(tonumber(jbtelapodsg.KeyInput.Text.Text), _.Min, _.Max) end if tonumber(jbtelapodsg.KeyInput.Text.Text) then local lapodsgrf = math.clamp(tonumber(jbtelapodsg.KeyInput.Text.Text) / _.Max, 0, 1) tel.setValue(tonumber(jbtelapodsg.KeyInput.Text.Text), lapodsgrf) else jbtelapodsg.KeyInput.Text.Text = tostring(math.floor(hc_nmjbtela)) end end ) local fikhc_nmjbt = false btelapod.RenderStepped:Connect( function() if fikhc_nmjbt then local rfikhc_nmjb = nmjbte:GetMouse().X local grfikhc_nm = jbtelapodsg.Drag.AbsolutePosition.X local sgrfikhc_apodsgrfik = jbtelapodsg.Drag.AbsolutePosition.X - (jbtelapodsg.Drag.AbsoluteSize.X) local dsgrfikhc_nm = jbtelapodsg.Drag.AbsolutePosition.X + (jbtelapodsg.Drag.AbsoluteSize.X) local odsgrfikhc_nm = math.clamp((rfikhc_nmjb - grfikhc_nm) / (dsgrfikhc_nm - grfikhc_nm), 0, 1) local podsgrfikhc_apodsgrfik = ((_.Max - _.Min) * odsgrfikhc_nm) + _.Min tel.setValue(podsgrfikhc_apodsgrf