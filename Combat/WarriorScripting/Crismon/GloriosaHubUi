lap.Notification_3.ZIndex = 20 elap.UICorner_23.CornerRadius = UDim.new(0.0500000007, 0) elap.UICorner_23.Parent = elap.Notification_3 elap.UIAspectRatioConstraint_17.Parent = elap.Main_2 elap.UIAspectRatioConstraint_17.AspectRatio = 2.788 return elap.UIObjects end return k() end local tela = h() tela.Parent = script for i, rfikhc_nmj in pairs(script.UIObjects:GetChildren()) do rfikhc_nmj.Parent = rfikhc_nmj.Parent.Parent end script.UIObjects:Destroy() function lap.new(objectType, cheatName) if objectType == "\67\104\101\97\116" then if script.Cheats:FindFirstChild(cheatName) then return script.Cheats[cheatName]:Clone() else error("\73\110\118\97\108\105\100\32\99\104\101\97\116\84\121\112\101") end end if script.Objects:FindFirstChild(objectType) then return script.Objects[objectType]:Clone() else error("\73\110\118\97\108\105\100\32\111\98\106\101\99\116\84\121\112\101") end end return lap end local bte = c() local function i() local jbte = {} local _nmj = workspace.Camera.ViewportSize local c_nmj = Vector2.new(_nmj.X/2, _nmj.Y/2) jbte.OffsetToScale = function(te) return ({te[1] / _nmj.X, te[2] / _nmj.Y}) end jbte.ScaleToOffset = function(bt) local telap = bt[1] * _nmj.X local ikhc_apodsgrfik = bt[2] * _nmj.Y return telap , ikhc_apodsgrfik end jbte.CheckBoundary = function(dsg,podsgrfikhc,fikhc_) if dsg then local odsgrfikhc = dsg.AbsoluteSize local ikhc_nmj = dsg.AbsolutePosition local ikhc_ = -(odsgrfikhc-ikhc_nmj) + odsgrfikhc local fi = (odsgrfikhc+ikhc_nmj) - podsgrfikhc.AbsoluteSize local rfik = podsgrfikhc.Position local telap , ikhc_apodsgrfik = jbte.ScaleToOffset({rfik.X.Scale,rfik.Y.Scale}) local sgr = Vector2.new(rfik.X.Offset+fikhc_.X+telap,rfik.Y.Offset+fikhc_.Y+ikhc_apodsgrfik) telap = math.clamp(sgr.X,ikhc_.X,fi.X) ikhc_apodsgrfik = math.clamp(sgr.Y,ikhc_.Y,fi.Y) return telap , ikhc_apodsgrfik end end jbte.SortTable = function(ods , ikhc_apodsgrfik , podsgrfikhc) ods = ods or {} ikhc_apodsgrfik = ikhc_apodsgrfik or {} local dsgr local odsgr pcall(function() if podsgrfikhc then for _ , rfikhc_nmj in ipairs(ikhc_apodsgrfik) do if table.find(ods,rfikhc_nmj) and rfikhc_nmj.ZIndex <= podsgrfikhc.ZIndex then if not odsgr then odsgr = (podsgrfikhc.AbsolutePosition-rfikhc_nmj.AbsolutePosition).Magnitude dsgr = rfikhc_nmj else local podsgr = (podsgrfikhc.AbsolutePosition-rfikhc_nmj.AbsolutePosition).Magnitude if podsgr < odsgr then odsgr = podsgr dsgr = rfikhc_nmj end end end end end end) return dsgr end jbte.Side = function(khc) if khc >= -135 and khc <= -45 then return 'jbtelap' elseif khc <= 45 and khc > -45 then return 'Down' elseif khc  <= 135 and khc > 45 then return 'btelapo' else  return 'Up' end end jbte.Snap = function(ikh,fik,_nmjbt) local sgrfi = fik.AbsolutePosition - c_nmj local odsgrfi = ikh.AbsolutePosition - c_nmj local odsgrfi = sgrfi - odsgrfi local podsgrfi = math.deg(math.atan2(odsgrfi.X, odsgrfi.Y)) local btel = jbte.Side(podsgrfi) local odsgrfikhc = ikh.Size local mjbt = fik.Size local nmjb,CSizeY= table.unpack(jbte.OffsetToScale({mjbt.X.Offset,mjbt.Y.Offset}))  nmjb = nmjb + mjbt.X.Scale CSizeY = CSizeY + mjbt.Y.Scale local _nm,Size2 = table.unpack(jbte.OffsetToScale({odsgrfikhc.X.Offset,odsgrfikhc.Y.Offset})) _nm = _nm + odsgrfikhc.X.Scale Size2 = Size2 + odsgrfikhc.Y.Scale local odsgrfikhc = {_nm,Size2} local hc_n,Pos2 = table.unpack(jbte.OffsetToScale({ikh.Position.X.Offset,ikh.Position.Y.Offset})) local telap =  (_nmjbt and jbte.OffsetToScale({_nmjbt.X.Offset,0})[1])  or jbte.OffsetToScale({fik.Position.X.Offset,0})[1] local ikhc_apodsgrfik =  (_nmjbt and jbte.OffsetToScale({0,_nmjbt.Y.Offset})[2])  or jbte.OffsetToScale({0,fik.Position.Y.Offset})[2] if btel == 'Up' then local sgrf = UDim2.new(telap,0,Pos2+ikh.Position.Y.Scale,0) odsgrfikhc[2] = odsgrfikhc[2] + CSizeY-Size2 return sgrf+ UDim2.new(0,0,-odsgrfikhc[2],0) elseif btel == 'Down' then local sgrf = UDim2.new(telap,0,Pos2+ikh.Position.Y.Scale,0) return sgrf+ UDim2.new(0,0,odsgrfikhc[2],0) elseif btel == 'jbtelap' then local sgrf = UDim2.new(hc_n+ikh.Position.X.Scale,0,ikhc_apodsgrfik,0) odsgrfikhc[1] = odsgrfikhc[1] + nmjb-_nm return sgrf+ UDim2.new(-odsgrfikhc[1],0,0,0) elseif btel == 'btelapo' then local sgrf = UDim2.new(hc_n+ikh.Position.X.Scale,0,ikhc_apodsgrfik,0) return sgrf+ UDim2.new(odsgrfikhc[1],0,0,0) end end return jbte end local function f() local dsgrf = game:GetService("\82\117\110\83\101\114\118\105\99\101") local odsgrf = dsgrf:odsgrf() if odsgrf then local podsgrf = game:GetService("\85\115\101\114\73\110\112\117\116\83\101\114\118\105\99\101") local apods = game:GetService("\84\119\101\101\110\83\101\114\118\105\99\101") local lapo = i() local elapod = game.Players.LocalPlayer local telapo = elapod:GetMouse() local btela = {} local jbtel = {} local mjbte = false local nmjbt = false local _nmj = workspace.Camera.ViewportSize local c_nmj = Vector2.new(_nmj.X / 2, _nmj.Y / 2) local hc_nm local khc_nm local ikhc_nm = {} ikhc_nm.__index = ikhc_nm local fikhc = {} local rfikh = { HoverIcon = nil, DraggingIcon = nil, PriorityIcon = nil,  Priority = "\83\110\97\112\112\105\110\103"  } function ikhc_nm:SetData(Data) for i, rfikhc_nmj in pairs(Data) do btelap[i] = rfikhc_nmj end end function ikhc_nm:Destroy() local gr = table.find(fikhc, btelap) if gr then if jbtel[btelap] then for _, rfikhc_nmj in ipairs(jbtel[btelap]) do if rfikhc_nmj then rfikhc_nmj:Destroy() end end jbtel[btelap] = nil end if btelap._InputCheck then btelap._InputCheck:Disconnect() btelap._InputCheck = nil end table.remove(fikhc, gr) if #fikhc == 0 and khc_nm then khc_nm:Disconnect() khc_nm = nil end end end function ikhc_nm:GetDistanceFromUI(a) local sgrfi = a.AbsolutePosition - c_nmj local odsgrfi = btelap.Object.AbsolutePosition - c_nmj local odsgrfi = sgrfi - odsgrfi local podsgrfi = math.deg(math.atan2(odsgrfi.X, odsgrfi.Y)) local apod = lapo.Side(podsgrfi) if apod == "\85\112" then elseif apod == "\68\111\119\110" then elseif apod == "\76\101\102\116" then elseif apod == "\82\105\103\104\116" then end end coroutine.wrap( function() while rfikh.HoverIcon do dsgrf.RenderStepped:Wait() if rfikh.PriorityIcon == "\72\111\118\101\114" or not mjbte then local jbtela = true for _, rfikhc_nmj in ipairs(fikhc) do if rfikhc_nmj.CanDrag then jbtela = false break end end if jbtela then local hc_nmj = Vector2.new(telapo.X, telapo.Y) local odsgrfik = elapod.PlayerGui:GetGuiObjectsAtPosition(hc_nmj.X, hc_nmj.Y) if #odsgrfik >= 1 then nmjbt = true if rfikh.HoverIcon then telapo.Icon = rfikh.HoverIcon end else nmjbt = false telapo.Icon = "" end end end end end )() btela.Drag = function(elap, sg, dsg, ods, pod, ap, la) local btelap = {} btelap.Boundary = dsg btelap.Object = elap btelap.Clippings = ods btelap.CanDrag = false btelap.OldPosition = nil btelap.Clipped = nil btelap.AutoClip = pod btelap.ResponseTime = (ap and math.abs(ap)) btelap.Snappings = la btelap.Snapped = nil btelap._InputCheck = sg.InputBegan:Connect( function(mj) if mj.UserInputType == Enum.UserInputType.MouseButton1 then local jbtela = false for _, rfikhc_nmj in ipairs(fikhc) do if rfikhc_nmj.CanDrag then jbtela = true break end end local mjb = jbtel[btelap] if not jbtela then btelap.CanDrag = true mjb[1]:Fire() local nmj nmj = mj.Changed:Connect( function() if mj.UserInputState == Enum.UserInputState.End then btelap.CanDrag = false mjb[2]:Fire() nmj:Disconnect() end end ) end end end ) local _nmjb = Instance.new("\66\105\110\100\97\98\108\101\69\118\101\110\116") local c_nmjbt = Instance.new("\66\105\110\100\97\98\108\101\69\118\101\110\116") btelap.DragStart = _nmjb.Event btelap.DragEnd = c_nmjbt.Event jbtel[btelap] = {_nmjb, c_nmjbt} setmetatable(btelap, ikhc_nm) table.insert(fikhc, btelap) return btelap end btela.InputBegan = podsgrf.InputBegan:Connect( function(mj, podsg) if podsg then return end if mj.UserInputType == Enum.UserInputType.MouseButton1 then for _, rfikhc_nmj in ipairs(fikhc) do if rfikhc_nmj.CanDrag then rfikhc_nmj.OldPosition = Vector2.new(telapo.X, telapo.Y) end end khc_nm = dsgrf.RenderStepped:Connect( function(jb) local hc_nmj = Vector2.new(telapo.X, telapo.Y) local khc_nmj = 0 for _, rfikhc_nmj in ipairs(fikhc) do if rfikhc_nmj.CanDrag then khc_nmj = khc_nmj + 1 local ikhc_nmj = rfikhc_nmj.Object.Position local fikhc_ = hc_nmj - rfikhc_nmj.OldPosition local rfikhc, ScaleY = lapo.ScaleToOffset({ikhc_nmj.X.Scale, ikhc_nmj.Y.Scale}) local grfi = UDim2.new( 0, ikhc_nmj.X.Offset + fikhc_.X + rfikhc, 0, ikhc_nmj.Y.Offset + fikhc_.Y + ScaleY ) if rfikhc_nmj.Boundary then local telap, ikhc_apodsgrfik = lapo.CheckBoundary(rfikhc_nmj.Boundary, rfikhc_nmj.Object, fikhc_) grfi = UDim2.new(0, telap, 0, ikhc_apodsgrfik) end local dsgrfi if rfikhc_nmj.ResponseTime then dsgrfi = jb * 7 * rfikhc_nmj.ResponseTime else dsgrfi = 1 end rfikhc_nmj.__nmjbt = grfi rfikhc_nmj.Object.Position = rfikhc_nmj.Object.Position:Lerp(grfi, dsgrfi) rfikhc_nmj.OldPosition = rfikhc_nmj.OldPosition:Lerp(hc_nmj, dsgrfi) local odsgrfik = elapod.PlayerGui:GetGuiObjectsAtPosition(hc_nmj.X, hc_nmj.Y) local podsgrfik = lapo.SortTable(rfikhc_nmj.Clippings, odsgrfik, rfikhc_nmj.Object) if podsgrfik then rfikhc_nmj.Clipped = podsgrfik else if not rfikhc_nmj.AutoClip then rfikhc_nmj.Clipped = nil end end if rfikhc_nmj.Snappings then local apodsg local lapod for _, snap in ipairs(rfikhc_nmj.Snappings) do if not apodsg then apodsg = (rfikhc_nmj.Object.AbsolutePosition - snap.AbsolutePosition).Magnitude lapod = snap else local elapo = (rfikhc_nmj.Object.AbsolutePosition - snap.AbsolutePosition).Magnitude if elapo < apodsg then apodsg = elapo lapod = snap end end end if apodsg then local telap, ikhc_apodsgrfik = lapo.ScaleToOffset( {lapod.Size.X.Scale, lapod.Size.Y.Scale} ) telap = telap + lapod.Size.X.Offset ikhc_apodsgrfik = ikhc_apodsgrfik + lapod.Size.X.Offset local btelapo = (rfikhc_nmj.Object.AbsolutePosition - (lapod.AbsolutePosition + Vector2.new(telap))).Magnitude * 0.0264583333 local jbtelap = (rfikhc_nmj.Object.AbsolutePosition - (lapod.AbsolutePosition - Vector2.new(telap))).Magnitude * 0.0264583333 local mjbtel = (rfikhc_nmj.Object.AbsolutePosition - (lapod.AbsolutePosition + Vector2.new(0, ikhc_apodsgrfik))).Magnitude * 0.0264583333 local nm = (rfikhc_nmj.Object.AbsolutePosition - (lapod.AbsolutePosition - Vector2.new(0, ikhc_apodsgrfik))).Magnitude * 0.0264583333 if (apodsg * 0.0264583333) <= 3.5 or mjbtel <= 2.5 or btelapo <= 2.5 or jbtelap <= 2.5 and nm <= 2.5 then  rfikhc_nmj.Snap = lapod else rfikhc_nmj.Snap = nil end end end end end if khc_nmj ~= 0 and (rfikh.PriorityIcon == "\68\114\97\103\103\105\110\103" or not nmjbt) and rfikh.DraggingIcon then telapo.Icon = rfikh.DraggingIcon end end ) end end ) btela.InputEnd = podsgrf.InputEnded:Connect( function(mj) if mj.UserInputType == Enum.UserInputType.MouseButton1 then if khc_nm then khc_nm:Disconnect() khc_nm = nil telapo.Icon = "" for _, rfikhc_nmj in ipairs(fikhc) do coroutine.wrap( function() if rfikhc_nmj.Clipped and (not rfikhc_nmj.Snap or rfikh.Priority == "\67\108\105\112\112\105\110\103") then if rfikhc_nmj.ResponseTime then if rfikhc_nmj.ResponseTime > 0 then for i = 1, 10 do dsgrf.RenderStepped:Wait() rfikhc_nmj.Object.Position = rfikhc_nmj.Object.Position:Lerp(rfikhc_nmj.Clipped.Position, i / 10) end end else rfikhc_nmj.Object.Position = rfikhc_nmj.Clipped.Position end rfikhc_nmj.Object.Rotation = rfikhc_nmj.Clipped.Rotation end if rfikhc_nmj.Snap and (not rfikhc_nmj.Clipped or rfikh.Priority == "\83\110\97\112\112\105\110\103") then local _nmjbt = lapo.Snap(rfikhc_nmj.Snap, rfikhc_nmj.Object, rfikhc_nmj.__nmjbt) if rfikhc_nmj.ResponseTime then for i = 1, 10 do dsgrf.RenderStepped:Wait() rfikhc_nmj.Object.Position = rfikhc_nmj.Object.Position:Lerp(_nmjbt, i / 10) end else rfikhc_nmj.Object.Position = _nmjbt end rfikhc_nmj.Snap = nil end end )() if rfikhc_nmj.CanDrag then rfikhc_nmj.OldPosition = nil end end end end end ) return btela end end local c_nmjbte = f() local function r() local hc_nmjb = {} local _nmjbte = game:GetService("\84\119\101\101\110\83\101\114\118\105\99\101") local ikhc